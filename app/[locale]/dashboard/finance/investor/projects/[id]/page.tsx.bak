// Mapping dari ID numerik di URL ke UUID di database
const PROJECT_ID_MAPPING: Record<string, string> = {
  "1": "17a97b56-a525-4c65-b627-2e1e9e3ce343", // Pulang Pisau
  "2": "db56f3d7-60c8-42a6-aff1-2220b51b32de", // Gunung Mas  
  "3": "61f9898e-224a-4841-9cd3-102f8c387943", // Kapuas
  "4": "a71ef98b-4213-41cc-8616-f450aae8889d", // Katingan
};

// Mapping nama kabupaten berdasarkan project
const KABUPATEN_MAPPING: Record<string, string> = {
  "17a97b56-a525-4c65-b627-2e1e9e3ce343": "Pulang Pisau",
  "db56f3d7-60c8-42a6-aff1-2220b51b32de": "Gunung Mas",
  "61f9898e-224a-4841-9cd3-102f8c387943": "Kapuas",
  "a71ef98b-4213-41cc-8616-f450aae8889d": "Katingan",
};

async function getCarbonProject(id: string): Promise<ProjectData | null> {
  const supabase = await createClient()
  
  // Convert numeric ID to UUID if needed
  const projectId = PROJECT_ID_MAPPING[id] || id
  
  const { data, error } = await supabase
    .from("carbon_projects")
    .select("*")
    .eq("id", projectId)
    .single()

  if (error || !data) {
    console.error("Error fetching carbon project:", error)
    return null
  }

  return data as ProjectData
}

async function getFinancialData(projectId: string): Promise<FinancialData> {
  // Convert numeric ID to UUID if needed
  const dbId = PROJECT_ID_MAPPING[projectId] || projectId
  
  // Get area data directly from database
  let area = 0;
  try {
    // Use the new database-driven area data function
    const areaData = await getAreaDataFromDatabase();
    const projectAreaData = areaData[dbId];
    
    if (projectAreaData) {
      area = projectAreaData.total_area_ha;
    } else {
      // If not found in view, try to query carbon_projects table directly
      const supabase = await createClient();
      const { data: project } = await supabase
        .from("carbon_projects")
        .select("luas_total_ha, ps_id")
        .eq("id", dbId)
        .single();
      
      if (project?.luas_total_ha) {
        area = project.luas_total_ha;
      } else if (project?.ps_id) {
        // Calculate from perhutanan_sosial
        const { data: ps } = await supabase
          .from("perhutanan_sosial")
          .select("luas_ha")
          .eq("id", project.ps_id)
          .single();
        
        area = ps?.luas_ha || 0;
      }
    }
  } catch (error) {
    console.error('Error getting area data for financial calculation:', error);
    // Last resort: use fallback function
    try {
      const fallbackData = await getAreaDataFallback();
      area = fallbackData[dbId]?.total_area_ha || 0;
    } catch (fallbackError) {
      console.error('Even fallback failed:', fallbackError);
      area = 0;
    }
  }
  
  const average_per_ha = 5684603 // Rata-rata Rp 5.684.603 per ha
  const total_investment = Math.round(area * average_per_ha)
  const spent_amount = Math.round(total_investment * 0.6) // 60% spent
  const remaining_amount = total_investment - spent_amount

  return {
    total_investment,
    spent_amount,
    remaining_amount,
    average_per_ha
  }
}

async function getAreaDataFromDatabase(): Promise<Record<string, { total_area_ha: number, area_description: string }>> {
  try {
    // Primary: Use the existing API that we know works
    // This API already queries the v_carbon_projects_kabupaten_luas view internally
    const response = await fetch('http://localhost:3002/api/dashboard/kabupaten-carbon-luas', {
      cache: 'no-store' // Ensure fresh data
    });
    
    if (!response.ok) {
      throw new Error(`API responded with status: ${response.status}`);
    }
    
    const data = await response.json();
    
    if (!data.success) {
      throw new Error('API returned unsuccessful response');
    }
    
    const kabupatenData = data.data?.kabupaten_with_carbon_projects || [];
    
    const areaMap: Record<string, { total_area_ha: number, area_description: string }> = {};
    
    // Process each kabupaten with carbon projects
    kabupatenData.forEach((kab: any) => {
      // Find matching project ID based on kabupaten name
      const cleanKabupatenName = kab.nama.replace('Kabupaten ', '').trim();
      
      // Find project ID that matches this kabupaten
      const projectId = Object.keys(PROJECT_ID_MAPPING).find(
        key => {
          const uuid = PROJECT_ID_MAPPING[key];
          return KABUPATEN_MAPPING[uuid] === cleanKabupatenName;
        }
      );
      
      if (projectId) {
        const uuid = PROJECT_ID_MAPPING[projectId];
        if (uuid) {
          // Special handling for Pulang Pisau project (ID 1) - gabungan dua wilayah
          if (cleanKabupatenName === 'Pulang Pisau') {
            areaMap[uuid] = {
              total_area_ha: kab.luas_total_ha,
              area_description: `${kab.luas_total_ha.toLocaleString('id-ID')} ha (gabungan Kabupaten Pulang Pisau dan Kotamadya Palangka Raya)`
            };
          } else {
            areaMap[uuid] = {
              total_area_ha: kab.luas_total_ha,
              area_description: `${kab.luas_total_ha.toLocaleString('id-ID')} ha`
            };
          }
        }
      }
    });
    
    console.log('Successfully fetched area data from API:', Object.keys(areaMap).length, 'projects');
    return areaMap;
  } catch (error) {
    console.error('Error fetching area data from API:', {
      message: error instanceof Error ? error.message : 'Unknown error',
      stack: error instanceof Error ? error.stack : undefined,
      name: error instanceof Error ? error.name : 'Unknown',
      fullError: error
    });
    // Fallback to querying database tables directly
    try {
      console.log('Attempting database fallback...');
      return await getAreaDataFallback();
    } catch (fallbackError) {
      console.error('Database fallback also failed:', {
        message: fallbackError instanceof Error ? fallbackError.message : 'Unknown error',
        stack: fallbackError instanceof Error ? fallbackError.stack : undefined,
        name: fallbackError instanceof Error ? fallbackError.name : 'Unknown',
        fullError: fallbackError
      });
      throw fallbackError;
    }
  }
}

async function getAreaDataFallback(): Promise<Record<string, { total_area_ha: number, area_description: string }>> {
  try {
    const supabase = await createClient();
    
    // Get all carbon projects with their ps_id
    const { data: carbonProjects, error: cpError } = await supabase
      .from("carbon_projects")
      .select("id, ps_id, kabupaten");
    
    if (cpError) {
      console.error("Error fetching carbon projects for fallback:", cpError);
      throw cpError;
    }
    
    const areaMap: Record<string, { total_area_ha: number, area_description: string }> = {};
    
    // For each carbon project, calculate area from perhutanan_sosial
    for (const cp of carbonProjects || []) {
      let total_area_ha = 0;
      let area_description = "";
      
      if (cp.ps_id) {
        // Get area from perhutanan_sosial
        const { data: psData } = await supabase
          .from("perhutanan_sosial")
          .select("luas_ha, kabupaten_id")
          .eq("id", cp.ps_id)
          .single();
        
        total_area_ha = psData?.luas_ha || 0;
        area_description = `${total_area_ha.toLocaleString('id-ID')} ha`;
      }
      
      // Special handling for Pulang Pisau
      if (cp.id === "17a97b56-a525-4c65-b627-2e1e9e3ce343") {
        // Pulang Pisau is a combination of multiple PS areas
        // We'll use a simplified calculation
        total_area_ha = 27876; // From database view logic
        area_description = "27,876 ha (gabungan Kabupaten Pulang Pisau dan Kotamadya Palangka Raya)";
      }
      
      if (total_area_ha > 0) {
        areaMap[cp.id] = {
          total_area_ha,
          area_description
        };
      }
    }
    
    return areaMap;
  } catch (error) {
    console.error('Error in getAreaDataFallback:', error);
    // Ultimate fallback to hardcoded values (should rarely happen)
    return {
      "17a97b56-a525-4c65-b627-2e1e9e3ce343": { // Pulang Pisau - gabungan dua wilayah
        total_area_ha: 27876,
        area_description: "27,876 ha (gabungan Kabupaten Pulang Pisau dan Kotamadya Palangka Raya)"
      },
      "db56f3d7-60c8-42a6-aff1-2220b51b32de": { // Gunung Mas
        total_area_ha: 72800.99,
        area_description: "72,800.99 ha"
      },
      "61f9898e-224a-4841-9cd3-102f8c387943": { // Kapuas
        total_area_ha: 56771,
        area_description: "56,771 ha"
      },
      "a71ef98b-4213-41cc-8616-f450aae8889d": { // Katingan
        total_area_ha: 29239,
        area_description: "29,239 ha"
      }
    };
  }
}

// Deprecated: keeping for backward compatibility
async function getAreaData(): Promise<Record<string, { total_area_ha: number, area_description: string }>> {
  return await getAreaDataFromDatabase();
}

async function getCarbonData(project: ProjectData): Promise<CarbonData> {
  // Get kabupaten from mapping
  const kabupatenName = KABUPATEN_MAPPING[project.id] || "Tidak diketahui"
  
  // Get area data from API or fallback
  const areaData = await getAreaData();
  const projectAreaData = areaData[project.id];
  
  let total_area_ha = 0;
  let area_description = "";
  
  if (projectAreaData) {
    total_area_ha = projectAreaData.total_area_ha;
    area_description = projectAreaData.area_description;
  } else {
    // Fallback to database value if available
    total_area_ha = project.luas_total_ha || 0;
    area_description = `${total_area_ha.toLocaleString('id-ID')} ha`;
  }

  // Convert estimated_credits to CO2 storage (1 credit = 1 ton CO2)
  const estimated_co2_storage = project.estimated_credits || 0

  return {
    total_area_ha,
    estimated_co2_storage,
    standard: project.standar_karbon || "VCS",
    verification_status: project.verification_status || "verified",
    area_description
  }
}

export async function generateMetadata({
  params
}: {
  params: Promise<{ locale: string; id: string }>
}): Promise<Metadata> {
  const { locale, id } = await params
  const t = await getTranslations({ locale, namespace: 'dashboard' })
  
  const project = await getCarbonProject(id)
  
  return {
    title: project ? `${project.nama_project} - Investor Dashboard` : "Project Not Found",
    description: project ? `Detail investasi dan keuangan untuk ${project.nama_project}` : "Project tidak ditemukan",
  }
}

